<h1>Accounts</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Account Name</th>
    </tr>
  </thead>
  <tbody>
    <% @accounts.each do |account| %>
    <tr>
      <td><%= link_to account.name, account_path(account) %> </td>
      <td> <%= link_to "Edit", edit_account_path(account), class: "btn btn-primary btn-sm" %></td>
    </tr>
    <% end %>
  </tbody>
</table>